import java.util.Scanner;
public class Strings {
    public static void main(String args [] ){
        Scanner sc = new Scanner(System.in);// Creater new object;

            System.out.println("Enter a String");
            String input = sc.nextLine(); // Taking input from user;

            System.out.println("Enter another String");
            String input2 = sc.nextLine(); // Taking input from user;

            //Comparision between input1 and input2 String;

            if (input.compareTo(input2) == 0) {
                System.out.println("Strings are equal");
            } else {
                System.out.println("Strings are not equal");
            }
                }
}
